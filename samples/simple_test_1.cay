TreeDirectorySet SmallTreeDir {
    Names: r"test1",
    Structure: {
        layers: {
            A: Directory,
            B: Directory
        },
        edges: {
            F: File
        }
    }
}

fold "test": SmallTreeDir {
    A { name as a } => {
        B { name as b } => {
            F {name as f} => `{b}/{a}/{f}`
        }
    }
}
