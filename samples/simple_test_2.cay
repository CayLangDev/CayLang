DirectorySet SmallNumDir {
    Names: r"[123]"
}

TreeDirectorySet SmallTreeDir {
    Names: r"test_2",
    Structure: {
        layers: {
            A: SmallNumDir,
            B: Directory
        },
        edges: {
            F: File
        }
    }
}

fold "test/testbed/test_2": SmallTreeDir {
    A { name as a } => {
        B { name as b } => {
            F {name as f} => `{b}/{a}/{f}`
        }
    }
}
